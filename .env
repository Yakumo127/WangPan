# 数据库配置
MYSQL_HOST=filemanager-mysql
MYSQL_PORT=3306
MYSQL_DATABASE=enterprise_file_manager
MYSQL_USERNAME=filemanager
MYSQL_PASSWORD=filemanager_password

# Redis配置
REDIS_HOST=filemanager-redis
REDIS_PORT=6379
REDIS_PASSWORD=

# 后端配置
SERVER_PORT=8080
SPRING_PROFILES_ACTIVE=docker

# 前端配置
VITE_APP_API_BASE_URL=http://localhost:8888/api
VITE_APP_TITLE=企业文件管理系统

# JWT配置
JWT_SECRET=your-secret-key-here
JWT_EXPIRATION=86400

# 文件存储配置
UPLOAD_PATH=/app/uploads
MAX_FILE_SIZE=104857600

# 日志配置
LOG_LEVEL=INFO
LOG_PATH=/app/logs